generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Addon {
  id          String   @id @map("_id") @default(auto()) @db.ObjectId
  name        String
  price       Float
  author      String
  email       String
  description String
  url         String
  verified    Boolean @default(false)
  createdAt   DateTime @default(now())
}
